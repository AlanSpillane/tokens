{
  "checkbox": {
    "checked":{
		"#normal":{
			"background": "@theme-control-active-normal",
			"text": "@theme-text-primary-normal",
			"border": "@theme-control-active-normal"
		},
		"#hovered":{
			"background": "@theme-control-active-hover",
			"text": "@theme-text-primary-normal",
			"border": "@theme-control-active-hover"
		},
		"#pressed":{
			"background": "@theme-control-active-pressed",
			"text": "@theme-text-primary-normal",
			"border": "@theme-control-active-pressed"
		},
		"#disabled":{
			"background": "@theme-control-active-disabled",
			"text": "@theme-text-primary-disabled",
			"border": "@theme-control-active-disabled"
		}
	},
	"unchecked":{
		"#normal":{
			"background": "@theme-background-secondary-normal",
			"text": "@theme-text-primary-normal",
			"border": "@theme-text-primary-normal"
		},
		"#hovered":{
			"background": "@theme-background-secondary-hover",
			"text": "@theme-text-primary-normal",
			"border": "@theme-text-primary-normal"
		},
		"#pressed":{
			"background": "@theme-background-secondary-hover",
			"text": "@theme-text-primary-normal",
			"border": "@theme-text-primary-normal"
		},
		"#disabled":{
			"background": "@theme-background-secondary-hover",
			"text": "@theme-text-primary-disabled",
			"border": "@theme-background-secondary-hover"
		}
	}
  }
}