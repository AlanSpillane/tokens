{
  "sharewindow": {
    "comment": "Applied to share window",
    "figma": "https://www.figma.com/file/NaNrfXjygZtRgMfHAFHjsp/Components---Windows%2BWeb?node-id=9532%3A17790",

    "preview": {
      "#normal": {
        "active": "@theme-control-shareContent-active",
        "inactive": "@theme-control-shareContent-inactive"
      }
    },
    "border": {
      "#normal": {
        "selected": "@theme-outline-share-selected",
        "active": "@theme-outline-share-active",
        "inactive": "@theme-outline-share-inactive"
      }
    },
    "control": {
      "text": {
        "primary": "theme-text-button-normal"
      },
      "button": {
        "#normal": {
          "background": "@theme-button-share-normal",
          "text": "@theme-text-button-normal",
          "border": "@theme-outline-buttonshare-normal"
        },
        "#hovered": {
          "background": "@theme-button-share-hover",
          "text": "@theme-text-button-normal",
          "border": "@theme-outline-buttonshare-normal"
        },
        "#pressed": {
          "background": "@theme-button-share-pressed",
          "text": "@theme-text-button-normal",
          "border": "@theme-outline-buttonshare-normal"
        },
        "#disabled": {
          "background": "@theme-button-share-pressed",
          "text": "@theme-text-primary-disabled",
          "border": "@theme-outline-buttonshare-normal"
        },
        "active": {
          "#normal": {
            "background": "@theme-button-share-active-normal",
            "text": "@theme-text-button-normal",
            "border": "@theme-outline-buttonshare-normal"
          },
          "#hovered": {
            "background": "@theme-button-share-active-hover",
            "text": "@theme-text-button-normal",
            "border": "@theme-outline-buttonshare-normal"
          },
          "#pressed": {
            "background": "@theme-button-share-active-pressed",
            "text": "@theme-text-button-normal",
            "border": "@theme-outline-buttonshare-normal"
          },
          "#disabled": {
            "background": "@theme-button-share-pressed",
            "text": "@theme-text-primary-disabled",
            "border": "@theme-outline-buttonshare-normal"
          }
        }
      }
    }
  }
}
