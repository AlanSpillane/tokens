{
  "button": {
    "primary": {
      "background": {
        "#normal": "@color-black-alpha-95",
        "#hovered": "@color-black-alpha-80",
        "#pressed": "@color-black-alpha-70"
      },
      "text": "@color-white-alpha-95"
    },
    "secondary": {
      "background": {
        "#normal": "@color-black-alpha-00",
        "#hovered": "@color-black-alpha-30",
        "#pressed": "@color-black-alpha-40"
      },
      "border-color": "@color-black-alpha-90",
      "text": "@color-black-alpha-95"
    },
    "outline-join": {
      "background": "@color-black-alpha-00",
      "border-color": "@color-green-70",
      "text": "@color-green-70"
    },
    "outline-cancel": {
      "background": "@color-black-alpha-00",
      "border-color": "@color-red-70",
      "text": "@color-red-70"
    },
    "ghost": {
      "background": {
        "#normal": "@color-white-alpha-00",
        "#hovered": "@color-black-alpha-11",
        "#pressed": "@color-black-alpha-20"
      },
      "text": "@color-black-alpha-95"
    },
    "focusRing": "@color-blue-60",
    "disabled": {
      "background": "@color-black-alpha-20",
      "text": "@color-black-alpha-40"
    }
  }
}
