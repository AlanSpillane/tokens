{
  "theme": {
    "color": "@color-blue",
    "text": {
      "primary": {
        "normal": "@color-white-alpha-95",
        "inverted": "@color-black-alpha-95",
        "disabled": "@color-white-alpha-40"
      },
      "secondary": {
        "normal": "@color-white-alpha-70"
      },
      "error":{
        "normal":"@color-red-40",
        "hover":"@color-red-20",
        "active":"@color-red-20"
      },
      "success":{
        "normal":"@color-green-40",
        "hover":"@color-green-20",
        "active":"@color-green-20"
      },
      "warning":{
        "normal":"@color-yellow-40",
        "hover":"@color-yellow-20",
        "active":"@color-yellow-20"
      },
      "highlight": "@theme-color-80",
      "accent": {
        "normal": "@theme-color-40",
        "hover": "@theme-color-20",
        "active": "@theme-color-20"
      }
    },
    "background": {
      "primary": {
        "normal": "@color-white-alpha-00",
        "hover": "@color-white-alpha-05",
        "active": "@color-white-alpha-11"
      },
      "disabled": "@color-white-alpha-11",
      "solid":{
        "normal": "@color-gray-100",
        "secondary":{
          "normal":"@color-gray-95"
        },
        "tertiary":{
          "normal":"@color-gray-90"
        }
      },
      "secondary":{
        "normal":"@color-white-alpha-11",
        "hover":"@color-white-alpha-20",
        "active":"@color-white-alpha-30"
      },
      "accent":{
        "normal":"@theme-color-60"
      },
      "gradient": {
        "primary": "@theme-gradation-primary",
        "secondary": "@theme-gradation-secondary"
      },
      "warning": "@color-yellow-80",
      "error": "@color-red-80",
      "theme": "@theme-color-90",
      "success": "@color-green-80"
    },
    "overlay": {
      "primary": "@color-black-alpha-90",
      "secondary": "@color-black-alpha-40"
    },
    "button": {
      "primary": {
        "normal":"@color-white-alpha-95",
        "hover": "@color-white-alpha-80",
        "pressed": "@color-white-alpha-70",
        "disabled": "@color-white-alpha-20"
      },
      "secondary":{
        "normal":"@color-white-alpha-00",
        "hover": "@color-white-alpha-07",
        "pressed": "@color-white-alpha-20"
      },
      "inverted": {
        "normal":"@color-gray-100",
        "hover": "@color-gray-90",
        "pressed": "@color-gray-80"
      },
      "join":{
        "normal":"@color-green-60",
        "hover": "@color-green-70",
        "pressed": "@color-green-80"
      },
      "cancel":{
        "normal":"@color-red-60",
        "hover": "@color-red-70",
        "pressed": "@color-red-80"
      },
      "accent":{
        "normal":"@theme-color-60",
        "hover": "@theme-color-70",
        "pressed": "@theme-color-80"
      }
    },
    "outline": {
      "primary": {
        "normal": "@color-white-alpha-40"
      },
      "focus": "@theme-color-40",
      "secondary":{
        "normal": "@color-white-alpha-20"
      },
      "button": {
        "normal": "@color-white-alpha-30"
      },
      "input":{
        "normal": "@color-white-alpha-50",
        "active": "@color-white-alpha-90"
      },
      "join": "@color-green-40",
      "cancel": "@color-red-50",
      "disabled": "@color-white-alpha-20",
      "gradient":{
        "active": ["@color-cobalt-70", "@color-cyan-40", "@color-green-50"]
      }
    },
    "control": {
      "active": {
        "normal": "@theme-color-40",
        "hover": "@theme-color-50",
        "pressed": "@theme-color-60",
        "active": "@theme-color-60",
        "disabled":"@theme-color-90"
      },
      "inactive": {
        "normal": "@color-white-alpha-50",
        "hover": "@color-white-alpha-11",
        "pressed": "@color-white-alpha-20",
        "disabled": "@color-white-alpha-05"
      }
    },
    "presence": {
      "busy": "@color-yellow-40",
      "doNotDisturb": "@color-red-60",
      "active": "@color-green-50",
      "away": "@color-gray-30"
    },
    "whiteboard": {
      "stickyNote": {
        "yellow": "@color-yellow-30",
        "red": "@color-red-40",
        "purple": "@color-purple-40",
        "cyan": "@color-cyan-40",
        "mint": "@color-mint-40"
      }
    },
    "avatar":{
      "default":"@color-white-alpha-30",
      "cobalt":"@color-cobalt-70",
      "cyan":"@color-cyan-70",
      "gold":"@color-gold-70",
      "lime":"@color-lime-70",
      "mint":"@color-mint-70",
      "orange":"@color-orange-70",
      "pink":"@color-pink-70",
      "purple":"@color-purple-70",
      "slate":"@color-slate-70",
      "violet":"@color-violet-70"
    },
    "scrollbar": {
      "button": {
        "normal": "@color-white-alpha-20",
        "hovered": "@color-white-alpha-30",
        "pressed": "@color-white-alpha-40"
      },
      "arrow": {
        "background": {
          "normal": "@color-white-alpha-00",
          "hovered":"@color-white-alpha-11",
          "pressed":"@color-white-alpha-20"
        }
      },
      "background":{
        "secondary":{
          "ghost":"@color-white-alpha-00",
          "normal":"@color-white-alpha-11"
        }
      }
    },
    "illustrations":{
      "empty":{
        "primary":["@color-lime-40","@color-cobalt-50"],
        "secondary":["@color-cobalt-50","@color-violet-60"]
      },
      "onboarding":{
        "primary":["@color-yellow-30","@color-lime-40"],
        "secondary":["@color-red-50","@color-violet-60"],
        "tertiary":["@color-yellow-30","@color-orange-50"],
        "quartenary":["@color-lime-40","@color-cobalt-50"],
        "quinary":["@color-cobalt-50","@color-violet-60"]
      },
      "success":["@color-green-40","@color-lime-40"],
      "error":["@color-pink-50","@color-red-60"]
    }
  }
} 
