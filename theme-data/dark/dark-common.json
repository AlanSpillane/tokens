{
  "theme": {
    "text": {
      "primary": "@color-white-alpha-95",
      "secondary": "@color-white-alpha-70",
      "disabled": "@color-white-alpha-40",
      "highlight": "@color-blue-80",
      "accent": {
        "#normal": "@color-blue-40",
        "#hovered": "@color-blue-20",
        "#pressed": "@color-blue-20",
        "#focused": "@color-blue-30"
      },
      "error": "@color-red-70",
      "button": "@color-white-alpha-95",
      "success": "@color-green-70",
      "warning": "@color-yellow-70"
    },
    "button": {
      "ghost": {
        "#normal": "@color-white-alpha-00",
        "#hovered": "@color-white-alpha-11",
        "#pressed": "@color-white-alpha-20"
      },
      "primary": {
        "#normal": "@color-white-alpha-95",
        "#hovered": "@color-white-alpha-80",
        "#pressed": "@color-white-alpha-70"
      },
      "secondary": {
        "#normal": "@color-white-alpha-20",
        "#hovered": "@color-white-alpha-11",
        "#pressed": "@color-white-alpha-07"
      },
      "disabled": "@color-white-alpha-20"
    },
    "background": {
      "solid": {
        "primary": "@color-gray-100",
        "secondary": "@color-gray-95",
        "tertiary": "@color-gray-90"
      },
      "warning": "@color-yellow-80",
      "error": "@color-red-80",
      "theme": "@color-blue-80",
      "success": "@color-green-80",
      "item": {
        "#normal": "@color-white-alpha-00",
        "#hovered": "@color-white-alpha-05",
        "#active": "@color-white-alpha-11"
      }
    },
    "overlay": {
      "primary": "@color-black-alpha-90",
      "secondary": "@color-black-alpha-40"
    },
    "border": {
      "primary": "@color-black-alpha-40",
      "secondary": "@color-black-alpha-20",
      "focus": "@color-blue-60",
      "input": {
        "#normal": "@color-black-alpha-50",
        "#active": "@color-black-alpha-90"
      }
    },
    "pageControl": {
      "#normal": "@color-white-alpha-50",
      "#active": "@color-white-alpha-90"
    },
    "control": {
      "#normal": "@color-blue-40",
      "#hovered": "@color-blue-50",
      "#pressed": "@color-blue-60",
      "#active": "@color-blue-60",
      "#disabled": "@color-blue-90"
    },
    "presence": {
      "busy": "@color-yellow-40",
      "doNotDisturb": "@color-red-60",
      "active": "@color-green-50",
      "away": "@color-gray-30"
    },
    "whiteboard": {
      "stickyNote": {
        "yellow": "@color-yellow-30",
        "red": "@color-red-40",
        "purple": "@color-purple-40",
        "cyan": "@color-cyan-30",
        "mint": "@color-mint-30"
      }
    }
  }
} 
