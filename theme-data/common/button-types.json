      "border-color": "@theme-button-outline-cancel-border-color",
      "text": "@theme-button-outline-cancel-text"
    },
    "ghost": {
      "background": "@theme-button-ghost-background",
      "text": "@theme-button-ghost-text"
    },
    "focusRing": "@theme-button-focusRing",
