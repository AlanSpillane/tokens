{
  "button": {
    "join": {
      "background": {
        "#normal": "@color-green-60",
        "#hovered": "@color-green-70",
        "#pressed": "@color-green-80"
      },
      "text": "@color-white-alpha-95"
    },
    "joinOutline": {
      "background": {
        "#normal": "@color-green-60",
        "#hovered": "@color-green-70",
        "#pressed": "@color-green-80"
      },
      "text": "@color-white-alpha-95"
    },
    "cancel": {
      "background": {
        "#normal": "@color-red-60",
        "#hovered": "@color-red-70",
        "#pressed": "@color-red-80"
      },
      "text": "@color-white-alpha-95"
    },
    "message": {
      "background": {
        "#normal": "@color-blue-60",
        "#hovered": "@color-blue-70",
        "#pressed": "@color-blue-80"
      },
      "text": "@color-white-alpha-95"
    },
    "screenShare": {
      "background": {
        "#normal": "@color-green-60",
        "#hovered": "@color-green-70",
        "#pressed": "@color-green-80"
      },
